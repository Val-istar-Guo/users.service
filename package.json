{
  "name": "user.service",
  "version": "0.0.1",
  "description": "nest example",
  "main": "index.js",
  "scripts": {
    "test": "ava",
    "test:debug": "ava --verbose",
    "test:update": "ava -u",
    "report": "nyc --reporter=html --reporter=text-summary --skip-full ava",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "start": "node dist/index.js",
    "ci": "git-cz",
    "ca": "git add -A && git-cz -av",
    "release": "standard-version --no-verify",
    "release:alpha": "standard-version --prerelease alpha",
    "release:first": "standard-version --first-release",
    "prepublishOnly": "npm test && npm run build && pinst --disable",
    "postpublish": "pinst --enable && git push",
    "clean": "rm -rf ./dist/*",
    "prebuild": "npm run clean",
    "build": "ttsc",
    "watch": "NODE_ENV=local node -r source-map-support/register ./build/watch.js",
    "postbuild": "npm run copyfile",
    "prewatch": "npm run copyfile",
    "copyfile": "npm run copyfile:js && npm run copyfile:dts",
    "copyfile:js": "copyfiles -e \"@(build|dist)/**/*.js\" **/*.js dist",
    "copyfile:dts": "copyfiles -e \"@(build|dist)/**/*.d.ts\" **/*.d.ts dist",
    "postinstall": "is-ci || husky install"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "^7.5.1",
    "@nestjs/core": "^7.5.1",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/swagger": "^4.7.2",
    "@nestjs/typeorm": "^7.1.4",
    "@types/bcrypt": "^3.0.0",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/node": "^14.14.7",
    "@types/ramda": "^0.27.32",
    "ali-oss": "^6.8.0",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "countries-list": "^2.5.4",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "ramda": "^0.27.1",
    "source-map-support": "^0.5.19",
    "ts-custom-error": "^3.1.1",
    "typeorm": "^0.2.29"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.0",
    "@commitlint/config-conventional": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "ava": "^3.13.0",
    "chalk": "^4.1.0",
    "commitizen": "^4.2.2",
    "copyfiles": "^2.4.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.13.0",
    "husky": "^5.1.1",
    "is-ci": "^3.0.0",
    "lint-staged": "^10.5.1",
    "mili": "^3.9.3",
    "nodemon": "^2.0.6",
    "pinst": "^2.1.6",
    "standard-version": "^9.0.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.0.5",
    "typescript-nestjs-swagger-plugin": "^1.1.0",
    "typescript-transform-paths": "^2.1.0"
  },
  "engines": {
    "node": ">=8.9.4"
  }
}
