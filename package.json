{
  "name": "user.service",
  "version": "0.0.1",
  "description": "nest example",
  "main": "index.js",
  "scripts": {
    "test": "ava",
    "test:debug": "ava --verbose",
    "test:update": "ava -u",
    "report": "nyc --reporter=html --reporter=text-summary --skip-full ava",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "start": "node lib/index.js",
    "ci": "git-cz",
    "ca": "git add -A && git-cz -av",
    "release": "standard-version --no-verify",
    "release:alpha": "standard-version --prerelease alpha",
    "release:first": "standard-version --first-release",
    "prepublishOnly": "npm test && npm run build",
    "postpublish": "git push",
    "clean": "rm -rf lib",
    "prebuild": "npm run clean",
    "build": "ttsc",
    "watch": "NODE_ENV=local node ./build/watch.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@nestjs/common": "^7.0.5",
    "@nestjs/core": "^7.0.3",
    "@nestjs/platform-express": "^7.0.6",
    "@nestjs/typeorm": "^7.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/node": "^13.9.5",
    "@types/ramda": "^0.27.3",
    "ali-oss": "^6.8.0",
    "ava": "^3.5.2",
    "bcrypt": "^4.0.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.0-rc.0",
    "commitizen": "^4.0.4",
    "countries-list": "^2.5.4",
    "cz-conventional-changelog": "^3.1.0",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1",
    "ramda": "^0.27.0",
    "ts-custom-error": "^3.1.1",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "chalk": "^3.0.0",
    "eslint": "^6.8.0",
    "lint-staged": "^10.1.3",
    "nodemon": "^2.0.2",
    "ttypescript": "^1.5.10",
    "typescript": "^3.8.3",
    "typescript-transform-paths": "^1.1.14"
  },
  "engines": {
    "node": ">=8.9.4"
  }
}
